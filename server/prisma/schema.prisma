generator client {
  provider = "prisma-client-js"
  output   = "../prisma-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id             Int @id @default(autoincrement())
  name           String
  email          String @unique
  hashedpassword String
}

model blacklist {
  jti String @id @unique
  expiresat DateTime
  createdat DateTime @default(now())
}